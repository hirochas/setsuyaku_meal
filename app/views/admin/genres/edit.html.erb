<div class= "h5 bg-secondary mt-5">ジャンル名編集</div>

<%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>

<div>ジャンル名</div>
<div><%= f.text_field :name, :size=>"30", class:"form-control mx-auto" %></div>

<div><%= f.submit "変更を保存", class:"btn btn-success"%></div>

<% end %>